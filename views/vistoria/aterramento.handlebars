<div style="width: 98%;padding-left: 2%;padding-top: 0.5%;">

    <a href='/gerenciamento/vistoriaPla/{{projeto._id}}' class='btn btn-sm botao-menu'><i
            class="bi bi-layers"></i>
        Planejamento</a>
    <a href='/gerenciamento/vistoriaAte/{{projeto._id}}' class='btn btn-sm botao-menu ativo'><i
            class="bi bi-layers"></i>
        Aterramento</a>
    <a href="/gerenciamento/vistoriaStb/{{projeto._id}}" class="btn btn-sm botao-menu"><i class="bi bi-layers"></i>
        StringBox</a>
    <a href="/gerenciamento/vistoriaInv/{{projeto._id}}" class="btn btn-sm botao-menu"><i class="bi bi-layers"></i>
        Inversor</a>
    <a href="/gerenciamento/vistoriaEst/{{projeto._id}}" class="btn btn-sm botao-menu"><i class="bi bi-layers"></i>
        Estruturas</a>
    <a href="/gerenciamento/vistoriaMod/{{projeto._id}}" class="btn btn-sm botao-menu"><i class="bi bi-layers"></i>
        Módulos</a>
    <a href='/gerenciamento/cronograma/{{projeto._id}}' class='btn btn-sm btn-primary'> <i
            class="bi bi-arrow-return-left"></i></a>        

    <div class="row" style="padding-left: 15px;">
        <div class="col">
            <div class="form-check">
                <input class='form-check-input' style="margin-top: 6px;" type="checkbox" name='ateDistancia'
                    {{vistoria.ateDistancia}}>
                <label class='col-form-label col-form-label-sm form-check-label'>Medir Distância</label>
            </div>
            <img id='fotoAteDistancia' name='fotoAteDistancia' style="width: 300px; height: 190px;" />
            <div class="row">
                <div class="col text-end">
                    <label class="col-form-label col-form-label-sm">Data Entrega:</label>
                </div>
                <div class="col">
                    <input name='dtAteDistancia' value='{{vistoria.dtAteDistancia}}' type="date" class='form-control form-control-sm'>
                </div>
            </div>
        </div>
        <div class="col">
            <div class="form-check">
                <input class='form-check-input' style="margin-top: 6px;" type="checkbox" name='ateFuracao'
                    {{vistoria.ateFuracao}}>
                <label class='col-form-label col-form-label-sm form-check-label'>Fazer buraco</label>
            </div>
            <img id='fotoAteFuracao' name='fotoAteFuracao' style="width: 300px; height: 190px;" />
            <div class="row">
                <div class="col text-end">
                    <label class="col-form-label col-form-label-sm">Data Entrega:</label>
                </div>
                <div class="col">
                    <input name='dtAteFuracao' value='{{vistoria.dtAteFuracao}}' type="date" class='form-control form-control-sm'>
                </div>
            </div>
        </div>
        <div class="col">
            <div class="form-check">
                <input class='form-check-input' style="margin-top: 6px;" type="checkbox" name='ateFixarHaste'
                    {{vistoria.ateFixarHaste}}>
                <label class='col-form-label col-form-label-sm form-check-label'>Fixar Haste</label>
            </div>
            <img id='fotoAteFixarHaste' name='fotoAteFixarHaste' style="width: 300px; height: 190px;" />
            <div class="row">
                <div class="col text-end">
                    <label class="col-form-label col-form-label-sm">Data Entrega:</label>
                </div>
                <div class="col">
                    <input name='dtAteFixarHaste' value='{{vistoria.dtAteFixarHaste}}' type="date" class='form-control form-control-sm'>
                </div>
            </div>
        </div>
        <div class="col">
            <div class="form-check">
                <input class='form-check-input' style="margin-top: 6px;" type="checkbox" name='ateConexao'
                    {{vistoria.ateConexao}}>
                <label class='col-form-label col-form-label-sm form-check-label'>Preparar Conexão do Inversor</label>
            </div>
            <img id='fotoAteConexao' name='fotoAteConexao' style="width: 300px; height: 190px;" />
            <div class="row">
                <div class="col text-end">
                    <label class="col-form-label col-form-label-sm">Data Entrega:</label>
                </div>
                <div class="col">
                    <input name='dtAteConexao' value='{{vistoria.dtAteConexao}}' type="date" class='form-control form-control-sm'>
                </div>
            </div>
        </div>
    </div>
</div>