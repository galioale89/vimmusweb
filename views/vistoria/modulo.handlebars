<div style="width: 98%;padding-left: 2%;padding-top: 0.5%;">

    <a href='/gerenciamento/vistoriaPla/{{projeto._id}}' class='btn btn-sm botao-menu ativo'><i
            class="bi bi-layers"></i>
        Planejamento</a>
    <a href='/gerenciamento/vistoriaAte/{{projeto._id}}' class='btn btn-sm botao-menu'><i class="bi bi-layers"></i>
        Aterramento</a>
    <a href="/gerenciamento/vistoriaStb/{{projeto._id}}" class="btn btn-sm botao-menu"><i
            class="bi bi-layers"></i>
        StringBox</a>
    <a href="/gerenciamento/vistoriaInv/{{projeto._id}}" class="btn btn-sm botao-menu"><i class="bi bi-layers"></i>
        Inversor</a>
    <a href="/gerenciamento/vistoriaEst/{{projeto._id}}" class="btn btn-sm botao-menu"><i class="bi bi-layers"></i>
        Estruturas</a>
    <a href="/gerenciamento/vistoriaMod/{{projeto._id}}" class="btn btn-sm botao-menu ativo"><i class="bi bi-layers"></i>
        Módulos</a>
    <a href='/gerenciamento/cronograma/{{projeto._id}}' class='btn btn-sm btn-primary'> <i
            class="bi bi-arrow-return-left"></i></a>        

    <div class="row" style="padding-left: 15px;">
        <div class="col-md-4">
            <div class="form-check">
                <input class='form-check-input' style="margin-top: 6px;" type="checkbox" name='modFixar'
                    {{vistoria.modFixar}}>
                <label class='col-form-label col-form-label-sm form-check-label'>Fixar Módulos</label>
            </div>
            <img id='fotoModFixar' name='fotoModFixar' style="width: 300px; height: 190px;" />
            <div class="row">
                <div class="col-md-4">
                    <label class="col-form-label col-form-label-sm">Data Entrega:</label>
                </div>
                <div class="col-md-5">
                    <input name='dtModFixar' value='{{vistoria.dtModFixar}}' type="date"
                        class='form-control form-control-sm'>
                </div>
            </div>
            <div class="form-check">
                <input class='form-check-input' style="margin-top: 6px;" type="checkbox" name='modCabos'
                    {{vistoria.modCabos}}>
                <label class='col-form-label col-form-label-sm form-check-label'>Passar Cabos</label>
            </div>
            <img id='fotoModCabos' name='fotoModCabos' style="width: 300px; height: 190px;" />
            <div class="row">
                <div class="col-md-4">
                    <label class="col-form-label col-form-label-sm">Data Entrega:</label>
                </div>
                <div class="col-md-5">
                    <input name='dtModCabos' value='{{vistoria.dtModCabos}}' type="date"
                        class='form-control form-control-sm'>
                </div>
            </div>
        </div>
        <div class="col-md-4">
            <div class="form-check">
                <input class='form-check-input' style="margin-top: 6px;" type="checkbox" name='modConexao'
                    {{vistoria.modConexao}}>
                <label class='col-form-label col-form-label-sm form-check-label'>Conexão das Placas</label>
            </div>
            <img id='fotoModConexao' name='fotoModConexao' style="width: 300px; height: 190px;" />
            <div class="row">
                <div class="col-md-4">
                    <label class="col-form-label col-form-label-sm">Data Entrega:</label>
                </div>
                <div class="col-md-5">
                    <input name='dtModConexao' value='{{vistoria.dtModConexao}}' type="date"
                        class='form-control form-control-sm'>
                </div>
            </div>
            <div class="form-check">
                <input class='form-check-input' style="margin-top: 6px;" type="checkbox" name='modCanaletas'
                    {{vistoria.modCanaletas}}>
                <label class='col-form-label col-form-label-sm form-check-label'>Fixar Canaletas</label>
            </div>
            <img id='fotoModCanaletas' name='dtModCanaletas' style="width: 300px; height: 190px;" />
            <div class="row">
                <div class="col-md-4">
                    <label class="col-form-label col-form-label-sm">Data Entrega:</label>
                </div>
                <div class="col-md-5">
                    <input name='dtModCanaletas' value='{{vistoria.dtModCanaletas}}' type="date"
                        class='form-control form-control-sm'>
                </div>
            </div>
        </div>
        <div class="col-md-4">
            <div class="form-check">
                <input class='form-check-input' style="margin-top: 6px;" type="checkbox" name='modAmarra'
                    {{vistoria.modAmarra}}>
                <label class='col-form-label col-form-label-sm form-check-label'>Fixar Fios</label>
            </div>
            <img id='fotoModAmarra' name='fotoModAmarra' style="width: 300px; height: 190px;" />
            <div class="row">
                <div class="col-md-4">
                    <label class="col-form-label col-form-label-sm">Data Entrega:</label>
                </div>
                <div class="col-md-5">
                    <input name='dtModAmarra' value='{{vistoria.dtModAmarra}}' type="date"
                        class='form-control form-control-sm'>
                </div>
            </div>
            <div class="form-check">
                <input class='form-check-input' style="margin-top: 6px;" type="checkbox" name='modConInv'
                    {{vistoria.modConInv}}>
                <label class='col-form-label col-form-label-sm form-check-label'>Conectar Inversor</label>
            </div>
            <img id='fotoModConInv' name='fotoModConInv' style="width: 300px; height: 190px;" />
            <div class="row">
                <div class="col-md-4">
                    <label class="col-form-label col-form-label-sm">Data Entrega:</label>
                </div>
                <div class="col-md-5">
                    <input name='dtModConInv' value='{{vistoria.dtModConInv}}' type="date"
                        class='form-control form-control-sm'>
                </div>
            </div>            
        </div>
    </div>
</div>